{
  "compilerOptions": {
    /*"target": "es2016",*/
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es6"
    ],
    "module": "Node16",
    "moduleResolution": "Node16",
    "noEmitOnError": true,
    "paths": {
      "@/*": [
        "cypress/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    /* Specify the base directory to resolve non-relative module names. */
    "types": [
      "node",
      "cypress"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}